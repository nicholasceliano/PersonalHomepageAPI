{
	"name": "personalhomepageapi",
	"version": "1.0.0",
	"description": "",
	"main": "/build/app.js",
	"scripts": {
		"start": "node build/app.js",
		"test": "mocha 'app/test/{,!(routes)/**}/*.test.js'",
		"testAPIs": "mocha 'app/test/routes/*.test.js'",
		"tsc": "tsc",
		"lint": "tslint 'app/**/*.ts' 'app/test/**/*.js'",
		"build_w": "npm run lint app/**/*.ts && rd \/s \/q \"./build\" && npm run tsc && npm run copy_data_to_build_folder",
		"build_w_prod": "npm run lint app/**/*.ts && rd \/s \/q \"./build\" && tsc --sourceMap false && npm run copy_data_to_build_folder",
		"restart_server": "ssh pi@192.168.0.200 sudo pm2 restart PersonalHomepageAPI",
		"copy_data_to_build_folder": "xcopy \/E \/I \".\/app\/data\" \".\/build\/data\"",
		"clean_server_folders": "ssh pi@192.168.0.200 sudo rm -rf \/var\/www\/PersonalHomepageAPI; mkdir \/var\/www\/PersonalHomepageAPI",
		"move_files_to_service": "scp -r .\/build pi@192.168.0.200:\/var\/www\/PersonalHomepageAPI && scp .\/package.json pi@192.168.0.200:\/var\/www\/PersonalHomepageAPI",
		"deploy_w": "npm run build_w_prod && npm run clean_server_folders && npm run move_files_to_service && ssh pi@192.168.0.200 cd \/var\/www\/PersonalHomepageAPI\/build\/; npm install && npm run restart_server"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/express": "^4.17.1",
		"@types/mysql": "^2.15.7",
		"@types/request": "^2.48.3",
		"express": "^4.17.1",
		"google-auth-library": "^3.1.2",
		"googleapis": "^43.0.0",
		"mysql": "^2.17.1",
		"opn": "^5.5.0",
		"request": "^2.88.0",
		"typescript": "^3.6.4",
		"uuid": "^3.3.3",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/uuid": "^3.4.5",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"mocha": "^6.2.1",
		"tslint": "^5.20.0"
	}
}
